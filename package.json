{
  "author": "Artem Ganev",
  "homepage": "https://artyom-88.github.io",
  "license": "GPL-3.0",
  "name": "artyom-88-admin",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyom-88/artyom-88-admin.git"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "scripts": {
    "build": "next build",
    "depcheck": "pnpm dlx depcheck",
    "lint": "next lint --max-warnings 1 --quiet",
    "lint:fix": "next lint --fix",
    "lint:warn": "next lint",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start:dev": "next dev -p 8080",
    "start:local": "export PORT=8080 && pnpm start:prod",
    "start:prod": "next start -p $PORT",
    "test": "echo 'Tests are not implemented yet'",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@nextui-org/react": "2.4.6",
    "@nextui-org/system": "2.2.5",
    "@nextui-org/theme": "2.2.9",
    "@tanstack/react-query": "5.52.2",
    "dayjs": "1.11.13",
    "formik": "2.4.6",
    "framer-motion": "11.3.30",
    "ky": "1.7.1",
    "next": "14.2.7",
    "react": "18.3.1",
    "react-datepicker": "6.9.0",
    "react-dom": "18.3.1",
    "yup": "1.4.0",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.28.11",
    "@types/node": "20.12.7",
    "@types/react": "18.2.74",
    "@types/react-datepicker": "6.2.0",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "sass": "1.74.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.4"
  },
  "pnpm": {
    "overrides": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
